    },
    "devDependencies": {
        "@types/node": "^20.11.14",
        "@vitejs/plugin-vue": "^4.2.3",
        "autoprefixer": "^10.4.17",
        "postcss": "^8.4.33",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.0.2",
        "vite": "^4.3.9",
        "vue-tsc": "^1.4.2"
    }
